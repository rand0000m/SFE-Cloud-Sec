module cloud-sec {
    yang-version 1;
    namespace "urn:opendaylight:params:xml:ns:yang:cloud-sec";
    prefix "cloud-sec";

    import service-function-forwarder { prefix sff; revision-date 2014-07-01; }
    import ietf-inet-types {prefix inet;}

    revision "2015-01-05" {
        description "Initial revision of cloud-sec model";
    }

    container service-function-registry {
        list service-function {
            key "name";
            leaf address {
                type inet:ip-address;
            }

            leaf name {
                type string;
            }
        }
    }

    container service-function-forwarder-registry {
        list service-function-forwarder {
            key "name";
            leaf address {
                type inet:ip-address;
            }

            leaf name {
                type string;
            }

            leaf ovs-bridge {
                type string;
            }

            list service-functions {
                key "sf-name";
                leaf sf-name {
                    type string;
                }
            }
        }
    }

    container networks {
        list network {
            key "addresses";

            leaf addresses {
                type inet:ip-prefix;
            }
        }
    }

    container rules {
        list rule {
            leaf action {
                type string;
            }

            leaf name {
                type string;
            }

            list actions {
                leaf name {
                    type string;
                }

                leaf "value" {
                    type string;
                }
            }

            list matches {
                leaf classifier {
                    type string;
                }

                leaf name {
                    type string;
                }
                list match {
                    leaf name {
                        type string;
                    }

                    leaf "value" {
                        type string;
                    }
                }
            }
        }
    }

    rpc create-tenant {
        input {
            leaf unused {
                type string;
            }
        }
        output {
            leaf unused {
                type string;
            }
        }
    }

    rpc create-endpoints {
        input {
            leaf unused {
                type string;
            }
        }
        output {
            leaf unused {
                type string;
            }
        }
    }

    rpc create-function-path {
        input {
            leaf unused {
                type string;
            }
        }
        output {
            leaf unused {
                type string;
            }
        }
    }

    rpc create-tunnel {
        input {
            leaf unused {
                type string;
            }
        }
        output {
            leaf unused {
                type string;
            }
        }
    }
}
